{
	"tasks": [
		{
			"type": "shell",
			"label": "build",
			"command": "/bin/bash",
			"args": [
				{
					"value": "make.sh",
					"quoting": "escape"
				},
				{
					"value": "${fileDirname}",
					"quoting": "escape"
				}
			],
			"options": {
				"cwd": "${workspaceFolder}/.vscode"
			},
			"problemMatcher": ["$gcc"],
			"group": "build",
			"detail": "Use this for SDL projects."
		}
	],
	"version": "2.0.0"
}
